{
  "environment": "development",
  "base_url": "http://localhost:8000",
  "frontend_url": "http://localhost:3000",
  "timeout": 30,
  "max_retries": 3,
  "parallel_execution": true,
  "debug_mode": true,
  "verbose_logging": true,
  "mock_external_services": true,
  "performance_thresholds": {
    "max_response_time": 10.0,
    "min_success_rate": 80.0,
    "max_memory_usage": 2048,
    "max_cpu_usage": 90.0,
    "max_concurrent_users": 50
  },
  "test_suites": {
    "health_check": {
      "enabled": true,
      "priority": 1,
      "timeout": 10,
      "critical": true
    },
    "frontend_tests": {
      "enabled": true,
      "priority": 2,
      "timeout": 60,
      "browser": "chrome",
      "headless": true
    },
    "api_tests": {
      "enabled": true,
      "priority": 3,
      "timeout": 30,
      "concurrent_requests": 10
    },
    "agent_tests": {
      "enabled": true,
      "priority": 4,
      "timeout": 120,
      "mock_llm": true
    },
    "workflow_tests": {
      "enabled": true,
      "priority": 5,
      "timeout": 180
    },
    "integration_tests": {
      "enabled": true,
      "priority": 6,
      "timeout": 300
    },
    "performance_tests": {
      "enabled": true,
      "priority": 7,
      "timeout": 600,
      "load_levels": [5, 10, 20, 50]
    },
    "data_tests": {
      "enabled": true,
      "priority": 8,
      "timeout": 60
    },
    "error_recovery_tests": {
      "enabled": true,
      "priority": 9,
      "timeout": 120
    }
  }
}